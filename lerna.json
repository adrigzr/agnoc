{
  "command": {
    "version": {
      "allowBranch": "master",
      "message": "chore(release): release [skip ci]"
    }
  },
  "ignoreChanges": [
    "**/.*",
    "**/*.md",
    "**/test/**"
  ],
  "npmClient": "yarn",
  "packages": [
    "packages/*"
  ],
  "useWorkspaces": true,
  "version": "0.1.0"
}
