{
  "command": {
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "message": "chore(release): release %v [skip ci]"
    }
  },
  "ignoreChanges": ["**/.*", "**/*.{md,json}", "**/test/**"],
  "npmClient": "yarn",
  "packages": ["packages/*"],
  "useWorkspaces": true,
  "version": "0.2.0"
}
